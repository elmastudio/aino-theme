					"fontFamily": "var(--wp--preset--font-family--arimo)"
				}
			},
			"serif": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--pt-serif)"
				}
			},
			"sans-serif": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--arimo)"
				}
			}
		},
