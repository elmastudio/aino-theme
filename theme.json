	"styles": {
		"spacing": {
			"blockGap": "0.0rem"
		},
		"color": {
			"background": "var(--wp--preset--color--main-background)",
			"text": "var(--wp--preset--color--font-secondary)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--text-xxxl)"
				},
				"color": {
					"text": "var(--wp--preset--color--font-primary)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--text-xxl)"
				},
				"color": {
					"text": "var(--wp--preset--color--font-primary)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--text-xl)"
				},
				"color": {
					"text": "var(--wp--preset--color--font-primary)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--text-l)"
				},
				"color": {
					"text": "var(--wp--preset--color--font-primary)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--text-m)"
				},
				"color": {
					"text": "var(--wp--preset--color--font-primary)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--text-s)"
				},
				"color": {
					"text": "var(--wp--preset--color--font-primary)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--arimo)",
			"lineHeight": "var(--wp--custom--line-height--body)",
			"fontSize": "var(--wp--preset--font-size--text-m)"
		},
		"blocks": {
			"core/heading": {
				"typography": {
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--text-l)",
					"fontWeight": "400"
				}
			},
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--button-color-primary)"
				}
			},
			"core/preformatted": {
				"color": {
					"background": "var(--wp--preset--color--code-background)",
					"text": "var(--wp--preset--color--code-color)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--text-s)"
				}
			}
		}
	},
